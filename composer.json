	"description": "Toolkit is a library that provides various helpers and classes to make the development of liman extensions easier.",
	"type": "library",
	"authors": [
		{
			"name": "Liman Authors",
			"email": "dogukan@liman.dev"
		}
	],
	"license": "MIT",
	"require": {
		"illuminate/validation": "^8.0|^9.0|^10.0|^11.0|^12.0",
		"illuminate/translation": "^8.0|^9.0|^10.0|^11.0|^12.0",
		"illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0",
		"vlucas/phpdotenv": "^5.6"
	},
	"autoload": {
